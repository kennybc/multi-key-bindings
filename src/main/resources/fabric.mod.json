{
  "schemaVersion": 1,
  "id": "multi-key-bindings",
  "version": "${version}",
  "name": "Multi Key Bindings",
  "description": "Allows players to create multiple keybindings for the same action/function.",
  "authors": [
    "kennybc"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/multi-key-bindings/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "us.kenny.MultiKeyBindingClient"
    ]
  },
  "mixins": [
    {
      "config": "multi-key-bindings.controlling.mixins.json",
      "environment": "client"
    },
    {
      "config": "multi-key-bindings.mixins.json",
      "environment": "client"
    }
  ],
  "accessWidener": "multi-key-bindings.accesswidener",
  "depends": {
    "fabricloader": ">=0.16.10",
    "minecraft": "=1.20.2",
    "java": ">=17",
    "fabric-api": "*"
  }
